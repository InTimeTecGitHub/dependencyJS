{
  "name": "dependencyjs",
  "version": "2.0.3",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "test": "node node_modules/nyc/bin/nyc --reporter=text node_modules/mocha/bin/_mocha --require ts-node/register -u tdd --timeout 999999 --colors ./test/**/*Spec.ts ./test/**/**/*Spec.ts",
    "coverage": "istanbul cover mocha -- --ui bdd -R spec -t 5000 --compilers js:babel-register",
    "install": "gulp tsc"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManishKumawat/dependencyJS.git"
  },
  "author": "Intimetec",
  "license": "ISC",
  "homepage": "https://github.com/ManishKumawat/dependencyJS.git#readme",
  "dependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.1",
    "chai": "^4.1.2",
    "chai-exclude": "^1.0.9",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "istanbul": "^0.4.5",
    "merge2": "^1.2.2",
    "mocha": "^5.2.0",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "nyc": "^13.1.0",
    "ts-node": "^7.0.1"
  },
  "keywords": [
    "dependency resolver",
    "IOC",
    "inject",
    "registry",
    "di",
    "dependency injection",
    "dependency inversion",
    "inversion of control container",
    "run time resolver",
    "unity container",
    "container"
  ]
}
